{
  "name": "tsnl",
  "version": "0.0.8",
  "author": "@forsigner",
  "bin": {
    "tsnl": "./bin/run"
  },
  "eslintConfig": {
    "extends": "leaf-app"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@types/jest": "^24.0.18",
    "cross-spawn": "^7.0.0",
    "eslint-config-leaf-app": "0.0.3",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/cross-spawn": "^6.0.0",
    "@types/eslint": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.5",
    "standard-version": "^7.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/template",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/leaf-team/tsnl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tsnl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "leaf-team/tsnl",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && npm run version",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "release": "standard-version"
  },
  "types": "lib/index.d.ts"
}
